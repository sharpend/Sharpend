<configuration>
	<toolbars>	  
		<toolbar id="toolbar1" class="DockApplication.MyDockApplication" title="Toolbar 1">
			<buttons>
			  <button eventargs="DockApplication.Window1" class="Sharpend.GtkSharp.CommandToolButton" assembly="Sharpend"  title="Window1" stock_id="gtk-save">
			    <delegates>
			      <delegate target="MainWindow" function="CreateWindow" event="Clicked">
				  </delegate>
			    </delegates>
				<!--
				<params>
	        		<param name="Windo" type="System.String">Hallo</param>
	      		</params>
	      		-->
			  </button>
			  
			  <button eventargs="DockApplication.SampleWindow" class="Sharpend.GtkSharp.CommandToolButton" assembly="Sharpend"  title="SampleWindow" stock_id="gtk-no">
			    <delegates>
			      <delegate target="MainWindow" function="CreateWindow" event="Clicked">
				  </delegate>
			    </delegates>
			    <!--
				<params>
	        		<param name="Windo" type="System.String">Hallo</param>
	      		</params>
	      		-->
			  </button>
			  
			  <button eventargs="Sharpend.GtkSharp.DockcontainerWidget" class="Sharpend.GtkSharp.CommandToolButton" assembly="Sharpend"  title="DockframeWindow" stock_id="gtk-no">
			    <delegates>
			      <delegate target="MainWindow" function="CreateWindow" event="Clicked">
				  </delegate>
	
			    </delegates>
			  </button>
			  
			  <button eventargs="DockApplication.SampleWindow" class="Sharpend.GtkSharp.CommandToolButton" assembly="Sharpend"  title="Testwindow" stock_id="gtk-no">
